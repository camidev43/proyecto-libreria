{
  "name": "react-library-template",
  "homepage": "https://morewings.github.io/react-library-template/",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean:lib": "rimraf dist",
    "build:lib": "vite build",
    "build": "run-s clean:lib build:lib",
    "lint:code": "eslint src/** --report-unused-disable-directives",
    "fix:code": "eslint src/** --report-unused-disable-directives --fix",
    "lint:types": "tsc --pretty --noEmit",
    "lint:style": "stylelint ./src/**/*.css",
    "fix:style": "run-s 'lint:style --fix'",
    "test": "run-s test:unit test:sb:ci --",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --reporter=dot",
    "test:sb": "vitest --browser",
    "test:sb:ci": "vitest run --browser --reporter=dot",
    "test:ci": "run-s test:unit:ci test:sb:ci --",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "start:docs": "storybook dev -p 6006",
    "build:docs": "storybook build",
    "generate:component": "npx generate-react-cli component",
    "prepare": "is-ci || husky",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-themes": "^9.1.2",
    "@storybook/addon-vitest": "9.1.2",
    "@storybook/builder-vite": "9.1.2",
    "@storybook/react": "9.1.2",
    "@storybook/react-vite": "9.1.2",
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "16.3.0",
    "@types/lodash": "4.17.20",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@yelo/rollup-node-external": "1.0.1",
    "alias-hq": "6.2.4",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-ssr-friendly": "1.3.0",
    "eslint-plugin-storybook": "9.1.1",
    "generate-react-cli": "8.4.9",
    "globals": "^16.3.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "playwright": "^1.54.2",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.2.4",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-docgen-typescript": "2.4.0",
    "react-dom": "19.1.0",
    "rimraf": "^6.0.1",
    "storybook": "9.1.2",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-order": "7.0.0",
    "stylelint-prettier": "5.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "7.1.0",
    "vite-plugin-dts": "4.5.4",
    "vitest": "^3.2.4",
    "zustand": "^5.0.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
